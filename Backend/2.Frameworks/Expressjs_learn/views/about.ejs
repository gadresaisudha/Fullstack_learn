   <% for (let i = 0;i<3;i++) { %>
    <h1><%=i%></h1>
    <% } %> 
    <%const data = [1,2,3,4,5] %> 
   <% for (const key in data) { %> 
    <h1><%=key %></h1>
    <% } %> 
</body>
</html>